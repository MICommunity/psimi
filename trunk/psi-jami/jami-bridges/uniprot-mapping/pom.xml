<?xml version="1.0"?>
<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
    <modelVersion>4.0.0</modelVersion>
    <parent>
        <groupId>psidev.psi.mi.jami.bridges</groupId>
        <artifactId>jami-bridges</artifactId>
        <version>1.1.1-SNAPSHOT</version>
    </parent>

    <groupId>psidev.psi.mi.jami.bridges</groupId>
    <artifactId>uniprot-mapping</artifactId>
    <version>1.1.1-SNAPSHOT</version>

    <name>PSI :: JAMI - uniprot-mapping</name>
    <description>JAMI module to map proteins to uniprot</description>

    <properties>
        <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
    </properties>

    <dependencies>

        <!-- Bridges Core -->
        <dependency>
            <groupId>psidev.psi.mi.jami.bridges</groupId>
            <artifactId>bridges-core</artifactId>
            <version>${pom.version}</version>
        </dependency>

        <!-- intact protein-mapping tool -->
        <dependency>
            <groupId>uk.ac.ebi.intact.tools</groupId>
            <artifactId>protein-mapping</artifactId>
            <version>1.0.3</version>
        </dependency>

        <!-- Unisave -->
        <dependency>
            <groupId>uk.ac.ebi.intact.bridges</groupId>
            <artifactId>intact-unisave</artifactId>
            <version>2.1.10.1</version>
        </dependency>

        <!-- Uniprot service -->
        <dependency>
            <groupId>uk.ac.ebi.uniprot.kraken</groupId>
            <artifactId>uniprotjapi</artifactId>
            <version>2015.02</version>
        </dependency>

    </dependencies>

    <repositories>
        <repository>
            <id>intact.nexus</id>
            <name>IntAct Nexus</name>
            <url>http://www.ebi.ac.uk/intact/maven/nexus/content/groups/public</url>
        </repository>
    </repositories>
</project>
